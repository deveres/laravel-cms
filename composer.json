{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "arrilot/laravel-widgets": "^3.13",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "creitive/breadcrumbs": "^3.3",
        "cviebrock/eloquent-sluggable": "^4.8",
        "dimsav/laravel-translatable": "^9.2",
        "doctrine/dbal": "^2.9",
        "encore/laravel-admin": "^1.7",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "jenky/laravel-plupload": "^2.0",
        "jxlwqq/composer-viewer": "^1.0",
        "jxlwqq/env-manager": "^0.1.0",
        "laravel-admin-ext/api-tester": "^1.0",
        "laravel-admin-ext/backup": "^1.0",
        "laravel-admin-ext/ckeditor": "^1.0",
        "laravel-admin-ext/config": "^1.0",
        "laravel-admin-ext/cropper": "^1.0",
        "laravel-admin-ext/helpers": "^1.0",
        "laravel-admin-ext/log-viewer": "^1.0",
        "laravel-admin-ext/media-manager": "^1.0",
        "laravel-admin-ext/phpinfo": "^1.0",
        "laravel-admin-ext/reporter": "^1.0",
        "laravel-admin-ext/scheduling": "^1.0",
        "laravel/browser-kit-testing": "^5.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "spatie/eloquent-sortable": "^3.4",
        "spatie/laravel-backup": "^5.12",
        "symfony/css-selector": "~4.3",
        "symfony/dom-crawler": "~4.3",
        "vakata/jstree": "^3.3",
        "vsch/laravel-translation-manager": "~2.6"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
