{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arrilot/laravel-widgets": "^3.9",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "creitive/breadcrumbs": "^3.2",
        "cviebrock/eloquent-sluggable": "^4.5",
        "dimsav/laravel-translatable": "^9.2",
        "doctrine/dbal": "^2.9",
        "encore/laravel-admin": "^1.6",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "jenky/laravel-plupload": "^2.0",
        "jxlwqq/composer-viewer": "^1.0",
        "jxlwqq/env-manager": "^0.1.0",
        "laravel-admin-ext/api-tester": "^1.0",
        "laravel-admin-ext/backup": "^1.0",
        "laravel-admin-ext/ckeditor": "^1.0",
        "laravel-admin-ext/config": "^1.0",
        "laravel-admin-ext/cropper": "^1.0",
        "laravel-admin-ext/helpers": "^1.0",
        "laravel-admin-ext/log-viewer": "^1.0",
        "laravel-admin-ext/media-manager": "^1.0",
        "laravel-admin-ext/phpinfo": "^1.0",
        "laravel-admin-ext/reporter": "^1.0",
        "laravel-admin-ext/scheduling": "^1.0",
        "laravel/browser-kit-testing": "^4.1",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "spatie/eloquent-sortable": "^3.4",
        "spatie/laravel-backup": "^5.11",
        "symfony/css-selector": "~4.2",
        "symfony/dom-crawler": "~4.2",
        "vakata/jstree": "^3.3",
        "vsch/laravel-translation-manager": "^2.5"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
